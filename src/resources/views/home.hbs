<div class="mt-4">
  <div class="row">

    {{#each blogs}}
    <div class="col-sm-6 col-lg-4">
      <div class="card card-blog-item">
        <img src="{{this.image}}" alt="{{this.name}}" />
        <div class="card-body">
          <h5 class="card-title">{{this.name}}</h5>
          <p class="card-text">{{this.description}}</p>
          <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
      </div>
    </div>
    {{/each }}
  </div>
</div>