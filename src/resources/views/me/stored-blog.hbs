<div class="mt-4">
  <h2>My Blog</h2>
  <table class="table table-hover mt-4">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Blog name</th>
        <th scope="col">Author</th>
        <th scope="col">Create at</th>
      </tr>
    </thead>
    <tbody>
      {{#each blogs}}
      <tr>
        <th scope="row">{{sum @index 1}}</th>
        <td>{{this.name}}</td>
        <td>{{this.author}}</td>
        <td>{{this.createdAt}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>